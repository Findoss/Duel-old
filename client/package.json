{
  "name": "duel-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "create": "sh tools/create.sh",
    "postinstall": "npm run build",
    "dev": "vue-cli-service serve --port 3002",
    "vue-inspect": "vue inspect > ../docs/build/webpack-config.js",
    "dev-storybook": "vue-cli-service serve:storybook -p 3004 -c tools/storybook",
    "build-storybook": "vue-cli-service build:storybook -c tools/storybook"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-plugin-unit-mocha": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "@vue/eslint-config-airbnb": "^3.0.3",
    "@vue/test-utils": "^1.0.0-beta.25",
    "debounce": "^1.2.0",
    "socket.io-client": "^2.1.1",
    "vue": "^2.5.17",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-i18n": "^8.4.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4",
    "vuex-socketio-plugin": "^0.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-knobs": "^4.0.11",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@storybook/addon-notes": "^4.0.11",
    "chai": "^4.1.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
